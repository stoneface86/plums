{
  "name": "plums",
  "version": "1.0.0",
  "description": "sass library used for Trackerboy",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch -I src demo/style.sass _site/css/style.css",
    "build:sass": "sass  -I src demo/style.sass _site/css/style.css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoneface86/plums.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stoneface86/plums/issues"
  },
  "homepage": "https://github.com/stoneface86/plums#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0"
  }
}
